{
  "presets": ["@babel/preset-env", "@babel/react"],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "react-intl",
      {
        "messagesDir": "./tmp/extractedMessages"
      }
    ]
  ],
    "env": {
        "production": {
            "plugins": [
                ["babel-plugin-jsx-remove-data-test-id", {
                    "attributes": ["data-testid", "data-test-id"]
                }]
            ]
        }
    }
}
