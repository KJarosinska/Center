{
  "name": "mudita-center",
  "version": "4.4.0",
  "description": "Mudita Center",
  "productName": "Mudita Center",
  "scripts": {
    "lint": "lerna run lint",
    "lint:typecheck": "lerna run lint:typecheck",
    "test:coverage": "lerna run test:coverage",
    "postinstall": "lerna bootstrap",
    "prerelease": "npm run app:prerelease",
    "APP COMMANDS": "=========================================================",
    "app:develop": "lerna run --scope app develop",
    "app:lint": "lerna run --scope app lint",
    "app:lint:typecheck": "lerna run --scope app lint:typecheck",
    "app:test:coverage": "lerna run --scope app test:coverage",
    "app:prerelease": "lerna run --scope app prerelease",
    "PURE COMMANDS": "========================================================",
    "pure:build": "lerna run --scope pure build",
    "pure:lint": "lerna run --scope pure lint",
    "pure:lint:typecheck": "lerna run --scope pure lint:typecheck",
    "pure:test:coverage": "lerna run --scope pure test:coverage",
    "pure:command": "lerna run --scope pure command --"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mudita/mudita-center"
  },
