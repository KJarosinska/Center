{
  "name": "pure-desktop-app",
  "version": "0.5.0",
  "description": "Pure Desktop App",
  "main": "./dist/main.js",
  "scripts": {
    "MAIN COMMANDS": "=========================================================",
    "develop": "npm run dev:start",
    "pack": "npm run build && electron-builder --dir --publish never",
    "release": "electron-builder --publish always --mac --win --linux",
    "sign-dev-electron-app": "codesign --deep --force --verbose --sign - node_modules/electron/dist/electron.app",
    "start": "electron .",
    "BUILD COMMANDS": "========================================================",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "cross-env NODE_ENV=production webpack --config webpack.config.main.js",
    "build:renderer": "cross-env NODE_ENV=production webpack --config webpack.config.renderer.js",
    "DEV COMMANDS": "==========================================================",
    "dev:start": "cross-env START_HOT=1 npm run dev:start:renderer",
    "dev:start:main": "webpack --config webpack.config.main.js && electron ./dist/main.js",
    "dev:start:renderer": "webpack-dev-server --config webpack.config.renderer.js --noBabelLoaderCache",
    "LINT COMMANDS": "=========================================================",
    "eslint": "eslint '{src,test,mocks}/**/*.{ts,tsx}'",
    "fix": "eslint . --fix",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint '{src,test,mocks}/**/*.{ts,tsx}'",
    "lint:js": "tslint '{src,test,mocks}/**/*.{ts,tsx}' --project ./tsconfig.json",
    "lint:typecheck": "tsc --noEmit",
    "DIST COMMANDS": "=========================================================",
    "dist": "npm run build && electron-builder --publish never",
    "dist:all": "npm run dist -- -mwl",
    "TEST COMMANDS": "=========================================================",
    "test": "jest '(/__tests__/.*|(\\.|/)(test|spec))\\.([tj]sx?)$'",
    "test:all": "npm t && npm run build",
    "test:coverage": "npm run test -- --coverage",
    "test:e2e": "jest '(/__tests__/.*|(\\.|/)(e2e))\\.([tj]sx?)$' --runInBand",
    "test:watch": "npm test -- --watch",
    "TRANSLATIONS COMMANDS": "=================================================",
    "translations:check": "ts-node -r esm ./scripts/translationCheck",
    "translations:generate": "ts-node -r esm ./scripts/translationRunner.js",
    "STORYBOOK COMMANDS": "====================================================",
    "storybook:serve": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "UTILITY/AUTO COMMANDS": "=================================================",
    "beforecommit": "lint-staged",
    "postinstall": "electron-builder install-app-deps && electron-rebuild -f",
    "prerelease": "npm run build",
    "prestart": "npm run build",
    "pretest:e2e": "npm run build"
  },
  "pre-commit": "beforecommit",
  "build": {
    "publish": {
      "provider": "s3",
      "bucket": "mudita-desktop-app",
      "path": "/app/"
    },
    "nsis": {
      "include": "installer.nsh",
      "artifactName": "PureDesktopApp-Setup-${version}.${ext}",
      "oneClick": true,
      "perMachine": false,
      "allowToChangeInstallationDirectory": false,
      "runAfterFinish": true,
      "deleteAppDataOnUninstall": true
    },
    "productName": "PureDesktopApp",
    "appId": "mudita.pure.desktopApp",
    "mac": {
      "category": "your.app.category.type"
    },
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "target": "AppImage"
    },
    "win": {
      "target": "nsis"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Appnroll/pure-desktop-app"
  },
  "author": {
    "name": "Mudita",
    "email": "contact@mudita.com"
  },
  "license": "SEE LICENSE",
  "bugs": {
    "url": "https://github.com/Appnroll/pure-desktop-app/issues"
  },
  "homepage": "https://github.com/Appnroll/pure-desktop-app",
